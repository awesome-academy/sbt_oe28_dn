<% provide :title, t("tours.index.title") %>
<div class="tour container index-tour">
  <h4><b><%= sort_link @search, :title, yield(:title) %><hr/></b></h4>
  <div class="search-form">
    <%= render "shared/search_form" %>
  </div>
  <br/>
  <div class="row">
    <%= render @tours %>
  </div>
  <div class="container">
    <%= will_paginate @tours %>
  </div>
</div>
